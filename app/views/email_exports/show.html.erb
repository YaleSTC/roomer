<% provide(:title, 'Email Export Results') %>
<h1>Email Export Results</h1>
<h2><%= draw_scope_str(@email_export) %><%= size_scope_str(@email_export) %><%= flag_scope_str(@email_export) %></h2>
<% if @email_export.users.empty? %>
  <p>No matching groups</p>
<% else %>
  <table role="presentation">
    <tbody>
      <% @email_export.users.each do |l| %>
        <tr>
          <td><%= l.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
<% if policy(@email_export).create? %>
  <%= link_to 'New email export', new_email_export_path, class: 'button secondary' %>
<% end %>
